pipeline {
  agent any 
  tools { maven 'maven' }
  stages {
    stage('build') {
      steps {
                bat 'mvn compile -f servicebackjava_docker/pom.xml'
                bat 'mvn package -f servicebackjava_docker/pom.xml'
      }
    }
    stage('test') {
      steps {
        bat 'java -jar servicebackjava/target/servicebackjava_docker-0.0.1-SNAPSHOT.jar'
      }
    }
  }
}
